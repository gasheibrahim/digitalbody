      <header class="site-navbar site-navbar-target" role="banner">

        <div class="container">
          <div class="row align-items-center position-relative">

            <div class="col-3 ">
              <div class="site-logo serif">
                <a href="index.html">I&M Acupuncture and Digital Body Message Center</a>
              </div>
            </div>

            <div class="col-9  text-right">
              
              <span class="d-inline-block d-lg-none"><a href="#" class="text-white site-menu-toggle js-menu-toggle py-5 text-white"><span class="icon-menu h3 text-white"></span></a></span>

              <nav class="site-navigation text-right ml-auto d-none d-lg-block" role="navigation">
                <ul class="site-menu main-menu js-clone-nav ml-auto ">
                  <% if user_signed_in? %>
                  <li><%= link_to "Home", root_url, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "About", about_index_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "Patience", patiences_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "Appointments", appointments_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li class="active"><%= link_to "New Patience", new_patience_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "New Appointment", new_appointment_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "Logout", destroy_user_session_path, method: :delete, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <% else %>
                  <li><%= link_to "Create An Account", new_user_registration_path, class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <li><%= link_to "Login", new_user_session_path, style:"color:steelblue", class:"nav-link", :"data-no-turbolink" => true, method: "get" %></li>
                  <% end %>
                </ul>
              </nav>
            </div>

            
          </div>
        </div>

      </header>

    <div class="ftco-blocks-cover-1">
      <div class="site-section-cover overlay" data-stellar-background-ratio="0.5" style="background-image: url('../assets/massage 3.jpg')">
        <div class="container">
          <div class="row align-items-center">
            <div class="col-md-7">
              <h1 class="mb-3">New Patience</h1>
              <p>This Page Helps You To Create A Patience.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <div class="site-section bg-light">
      <h2 class="text-center">New Patience</h2>
      <div class="container">
        <%= form_with(model: patience, local: true) do |form| %>
          <% if patience.errors.any? %>
            <div id="error_explanation">
              <h2><%= pluralize(patience.errors.count, "error") %> prohibited this patience from being saved:</h2>

              <ul>
              <% patience.errors.full_messages.each do |message| %>
                <li><%= message %></li>
              <% end %>
              </ul>
            </div>
          <% end %>
          <div class="form-group row">
            <div class="col-md-6 mb-4 mb-lg-0">
              <%= form.text_field :fullname, class:"form-control", class:"form-control", placeholder:"Full Name" %>
            </div>
            <div class="col-md-6">
              <%= form.select :sickness, options_for_select([
                  ["select Sickness", ""],
                  "Knee Pain",
                  "Blood Vessel disorders",
                  "Rheumatism",
                  "Erectile dysfunction", ]), 
                  {}, { class: "form form-group form-control"} %>
            </div>
          </div>
          <div class="form-group row">
            <div class="col-md-12">
              <%= form.text_area :medicines, class:"form-control", placeholder:"Write A medicine Of Patience", cols:"30", rows:"10" %>
            </div>
          </div>
          <div class="form-group">
            <%= form.hidden_field :user_id, value: current_user.id %>
          </div>
          <div class="form-group row">
            <div class="col-md-6 mr-auto">
              <%= form.submit class:"btn btn-block btn-primary text-white py-3 px-5" %>
            </div>
          </div>
        <% end %>
      </div>
    </div>

    <footer class="site-footer bg-image overlay" style="background-image: url('../assets/massage 4.jpg')">
      <div class="container">
